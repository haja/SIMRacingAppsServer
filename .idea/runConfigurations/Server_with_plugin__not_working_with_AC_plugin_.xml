<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="Server with plugin (not working with AC plugin)" type="Application" factoryName="Application">
    <option name="MAIN_CLASS_NAME" value="com.SIMRacingApps.Server" />
    <module name="SIMRacingAppsServer" />
    <option name="VM_PARAMETERS" value="-Xmx512m" />
    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value="com.SIMRacingApps.*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <method v="2">
      <option name="Make" enabled="true" />
      <option name="AntTarget" enabled="true" antfile="file://$PROJECT_DIR$/../SIMRacingAppsSIMPluginAC/build.xml" target="installAsPlugin" />
      <option name="AntTarget" enabled="true" antfile="file://$PROJECT_DIR$/build.xml" target="build" />
    </method>
  </configuration>
</component>